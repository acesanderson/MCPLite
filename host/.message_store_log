[34m    [0m[34m"uri"[0m[34m: [0m[34m"file://todos/2025-05-03"[0m
[34m  [0m[1;34m}[0m
[1;34m}[0m

[1;32mâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ [0mMessage[1;32m â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[0m
[1;36massistant:[0m
[1;34m{[0m
[34m  [0m[34m"meta"[0m[34m: null,[0m
[34m  [0m[34m"resource"[0m[34m: [0m[1;34m{[0m
